#!/usr/bin/env stest
//
// useful debuging tool: "? expression" - inspect variable
//

; $d = new Datetime();
? $d;
    'DateTime ';

#
# show Class Name, Parent Class Name, Filename (hides part of path common for the script and class filename)
#
? new Test();
    'Test init.php';
? new TestB();
    'TestB(Test) init.php';
? "TestB";
    'TestB(Test) init.php';

